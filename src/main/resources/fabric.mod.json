{
  "schemaVersion": 1,
  "id": "modmanager",
  "version": "${version}",
  "name": "Mod Manager",
  "description": "Extends ModMenu with an additional tab where you can install mods from Modrinth",
  "authors": [
	"DeathsGun"
  ],
  "contact": {
	"homepage": "https://github.com/DeathsGun/ModManager",
	"sources": "https://github.com/DeathsGun/ModManager",
	"issues": "https://github.com/DeathsGun/ModManager/issues"
  },
  "license": "APL-2.0",
  "environment": "client",
  "entrypoints": {
	"client": [
	  "xyz.deathsgun.modmanager.ModManager"
	]
  },
  "mixins": [
	"modmanager.mixins.json"
  ],
  "depends": {
	"fabricloader": ">=0.11.3",
	"modmenu": ">=${modmenu_version}",
	"minecraft": "1.17.x",
	"java": ">=16"
  }
}
